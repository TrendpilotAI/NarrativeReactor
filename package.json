{
  "name": "narrative-reactor",
  "version": "1.0.0",
  "description": "AI-powered content generation platform for Signal Studio",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "genkit start -- npx tsx --watch src/index.ts",
    "docs:serve": "mkdocs serve",
    "docs:build": "mkdocs build",
    "docs:deploy": "mkdocs gh-deploy --force",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results.xml"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.33.0",
    "@fal-ai/client": "^1.8.4",
    "@genkit-ai/dotprompt": "*",
    "@genkit-ai/express": "^1.27.0",
    "@genkit-ai/firebase": "*",
    "@genkit-ai/google-genai": "*",
    "@genkit-ai/vertexai": "*",
    "@types/cors": "^2.8.19",
    "@types/lodash": "^4.17.23",
    "@types/mime-types": "^3.0.1",
    "cors": "^2.8.6",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.6.1",
    "genkit": "*",
    "genkitx-anthropic": "*",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.2",
    "twitter-api-v2": "^1.29.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^3.2.4",
    "firebase-tools": "^15.3.0",
    "genkit-cli": "^1.27.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
